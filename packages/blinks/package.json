{
  "name": "@dialectlabs/blinks",
  "version": "0.22.4",
  "license": "Apache-2.0",
  "private": false,
  "sideEffects": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/dialectlabs/blinks"
  },
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./ext/twitter": {
      "import": "./dist/ext/twitter.js",
      "require": "./dist/ext/twitter.cjs",
      "types": "./dist/ext/twitter.d.ts"
    },
    "./hooks/solana": {
      "import": "./dist/hooks/solana/index.js",
      "require": "./dist/hooks/solana/index.cjs",
      "types": "./dist/hooks/solana/index.d.ts"
    },
    "./hooks/evm": {
      "import": "./dist/hooks/evm/index.js",
      "require": "./dist/hooks/evm/index.cjs",
      "types": "./dist/hooks/evm/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.cjs",
      "types": "./dist/api/index.d.ts"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@solana/actions-spec": "~2.4.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.39",
    "postcss-prefix-selector": "^1.16.1",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.3",
    "tsup": "^8.2.0",
    "typescript": "^5.5.3",
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/wallet-adapter-react-ui": "^0.9.0",
    "@solana/web3.js": "^1.95.1",
    "viem": "^2.x",
    "wagmi": "^2.x"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/wallet-adapter-react-ui": "^0.9.0",
    "@solana/web3.js": "^1.95.3",
    "react": ">=18",
    "react-dom": ">=18",
    "viem": "^2.x",
    "wagmi": "^2.x"
  },
  "dependencies": {
    "@dialectlabs/blinks-core": "^0.20.5",
    "bs58": "^6.0.0",
    "clsx": "^2.1.1",
    "markdown-to-jsx": "^7.5.0"
  },
  "peerDependenciesMeta": {
    "@solana/wallet-adapter-react": {
      "optional": true
    },
    "@solana/wallet-adapter-react-ui": {
      "optional": true
    },
    "@solana/web3.js": {
      "optional": true
    },
    "viem": {
      "optional": true
    },
    "wagmi": {
      "optional": true
    }
  }
}
